    "text": "string",
    "type": "module"
  }, {
    "id": 3,
    "text": "local function count(str, char)\r\n      local c = 0\r\n      local byte = string.byte\r\n      for i = 1, #str do\r\n      \t  if byte(str, i) == char then c = c + 1 end\r\n      end\r\n      return c\r\nend",
    "type": "function"
  }, {
    "id": 4,
    "text": "function new(str)\r\n  return { str = str, position = 1, line = 1, getc = getc,\r\n\t   backtrack = backtrack, gets = gets, state = state, \r\n           log_error = log_error, errors = {} }\r\nend",
    "type": "function"
  }, {
    "id": 5,
    "text": "function log_error(strm, err)\r\n  local err_data\r\n  if type(err) == \"table\" then\r\n    local position, line = err.state[1], err.state[2]\r\n    err_data = { msg = err.msg, position = position, line = line }\r\n  else\r\n    err_data = { msg = err, position = strm.position, line = strm.line }\r\n  end\r\n  table.insert(strm.errors, err_data)\r\nend",
    "type": "function"
  }, {
    "id": 6,
    "text": "function getc(strm)\r\n\t local current = strm.position\r\n\t if current > string.len(strm.str) then\r\n\t    return error(\"end of stream\")\r\n\t else\r\n\t    local c = string.sub(strm.str, current, current)\r\n\t    strm.position = current + 1\r\n\t    if c == \"\\n\" then strm.line = strm.line + 1 end\r\n\t    return c\r\n\t end\r\nend",
    "type": "function"
  }, {
    "id": 7,
    "text": "function gets(strm, l)\r\n\t local start, finish = strm.position, strm.position + l - 1\r\n\t if finish > string.len(strm.str) then\r\n\t    return error(\"end of stream\")\r\n\t else\r\n\t    strm.position = finish + 1\r\n\t    local s = string.sub(strm.str, start, finish)\r\n\t    strm.line = strm.line + count(s, string.byte(\"\\n\"))\r\n\t    return s\r\n\t end\r\nend",
    "type": "function"
  }, {
    "id": 8,
    "text": "function state(strm)\r\n  return { strm.position, strm.line }\r\nend",
    "type": "function"
  }, {
    "id": 9,
    "text": "function backtrack(strm, st)\r\n  strm.position = st[1]\r\n  strm.line = st[2]\r\nend",
    "type": "function"
  }, {
    "id": 10,
    "text": "",
    "type": "function container"
  }, {
    "id": 11,
    "text": "function string.concat (...)\r\n  return table.concat (arg)\r\nend",
    "type": "function"
  }, {
    "id": 12,
    "text": "return",
    "type": "statement:keyword"
  }, {
    "id": 13,
    "text": "",
    "type": "blank lines"
  }, {
    "id": 14,
    "text": "function string.caps (s)\r\n  return (string.gsub (s, \"(%w)([%w]*)\",\r\n                       function (l, ls)\r\n                         return string.upper (l) .. ls\r\n                       end))\r\nend",
    "type": "function"
  }, {
    "id": 15,
    "text": "",
    "type": "blank lines"
  }, {
    "id": 16,
    "text": "function string.chomp (s)\r\n  return (string.gsub (s, \"\\n$\", \"\"))\r\nend",
    "type": "function"
  }, {
    "id": 17,
    "text": "",
    "type": "blank lines"
  }, {
    "id": 18,
    "text": "function string.escapePattern (s)\r\n  return (string.gsub (s, \"(%W)\", \"%%%1\"))\r\nend",
    "type": "function"
  }, {
    "id": 19,
    "text": "",
    "type": "blank lines"
  }, {
    "id": 20,
    "text": "function string.escapeShell (s)\r\n  return (string.gsub (s, \"([ %(%)%\\\\%[%]\\\"'])\", \"\\\\%1\"))\r\nend",
    "type": "function"
  }, {
    "id": 21,
    "text": "",
    "type": "blank lines"
  }, {
    "id": 22,
    "text": "function string.ordinalSuffix (n)\r\n  n = math.mod (n, 100)\r\n  local d = math.mod (n, 10)\r\n  if d == 1 and n ~= 11 then\r\n    return \"st\"\r\n  elseif d == 2 and n ~= 12 then\r\n    return \"nd\"\r\n  elseif d == 3 and n ~= 13 then\r\n    return \"rd\"\r\n  else\r\n    return \"th\"\r\n  end\r\nend",
