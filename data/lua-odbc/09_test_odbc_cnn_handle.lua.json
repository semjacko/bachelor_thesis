    "type": "blank lines"
  }, {
    "id": 16,
    "text": "local function clone_cnn(cnn)\r\n  local h = assert_userdata(cnn:handle())\r\n  cnn2 = assert_userdata(odbc.init_connection(h))\r\n  assert_equal(h, cnn2:handle())\r\nend",
    "type": "function"
  }, {
    "id": 17,
    "text": "local h = assert_userdata(cnn:handle())",
    "type": "statement:localassign"
  }, {
    "id": 18,
    "text": "cnn2 = assert_userdata(odbc.init_connection(h))",
    "type": "statement:assign"
  }, {
    "id": 19,
    "text": "assert_equal(h, cnn2:handle())",
    "type": "statement:functioncall"
  }, {
    "id": 20,
    "text": "",
    "type": "blank lines"
  }, {
    "id": 21,
    "text": "function test_clone()\r\n  clone_cnn(cnn)\r\n  assert_true(cnn2:connected())\r\n  assert_true(cnn2:disconnect())\r\n  assert(not cnn2:connected())\r\n  assert(not cnn:connected())\r\nend",
    "type": "function"
  }, {
    "id": 22,
    "text": "clone_cnn(cnn)",
    "type": "statement:functioncall"
  }, {
    "id": 23,
    "text": "assert_true(cnn2:connected())",
    "type": "statement:functioncall"
  }, {
    "id": 24,
    "text": "assert_true(cnn2:disconnect())",
    "type": "statement:functioncall"
  }, {
    "id": 25,
    "text": "assert(not cnn2:connected())",
    "type": "statement:functioncall"
  }, {
    "id": 26,
