    "text": "function PingPong(thread,inp,out,message)\r\n  print(thread.name,\"started\")\r\n  while true do\r\n    local msg = thread:receive(inp)\r\n    print(thread.name .. \" receives '\" .. msg .. \"'\")\r\n    thread:send(out,message)\r\n  end\r\nend",
