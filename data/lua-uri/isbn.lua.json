    "id": 10,
    "text": "local function _normalize_isbn (isbn)\r\n    isbn = isbn:gsub(\"%-\", \"\"):upper()\r\n    local ISBN = Util.attempt_require(\"isbn\")\r\n    if ISBN then return tostring(ISBN:new(isbn)) end\r\n    return isbn\r\nend",
    "type": "function"
  }, {
    "id": 11,
    "text": "isbn = isbn:gsub(\"%-\", \"\"):upper()",
    "type": "statement:assign"
